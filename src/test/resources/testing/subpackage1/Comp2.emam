package testing.subpackage1;

component Comp2 {
    port
        in Q (0.0 : 0.1 : 100.0) in1,
        in Q (0.0 : 0.1 : 100.0) in2,

        out Q (0.0 : 0.1 : 100.0) out1;

    implementation Math {
        Q sqrIn1 = in1 * in1;
        Q sqrIn2 = in2 * in2;
        out1 = 100.0 * ( max(sqrIn1, sqrIn2) / (sqrIn1 + sqrIn2) );
    }
}
