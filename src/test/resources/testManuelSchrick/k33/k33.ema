package testManuelSchrick.k33;

component K33 {
	ports
	    in Integer inA1,
	    in Integer inA2,
	    in Integer inA3,
		out Integer outA1,
		out Integer outA2,
		out Integer outA3;

    component DummyA {
      ports
        in Integer in1,
        out Integer out1,
        out Integer out2,
        out Integer out3;
    }

    component DummyB {
      ports
        in Integer in1,
        in Integer in2,
        in Integer in3,
        out Integer out1;
    }

    instance DummyA dummyA1;
    instance DummyA dummyA2;
    instance DummyA dummyA3;
    instance DummyB dummyB1;
    instance DummyB dummyB2;
    instance DummyB dummyB3;

    connect dummyA1.out1 -> dummyB1.in1;
    connect dummyA1.out2 -> dummyB2.in1;
    connect dummyA1.out3 -> dummyB3.in1;

    connect dummyA2.out1 -> dummyB1.in2;
    connect dummyA2.out2 -> dummyB2.in2;
    connect dummyA2.out3 -> dummyB3.in2;

    connect dummyA3.out1 -> dummyB1.in3;
    connect dummyA3.out2 -> dummyB2.in3;
    connect dummyA3.out3 -> dummyB3.in3;

}
